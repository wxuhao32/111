#!/usr/bin/env sh
# Minimal Gradle Wrapper script (Unix/macOS/Linux)
# This project is primarily built by Flutter, but Gradle is used under android/.

set -e

APP_HOME=$(cd "$(dirname "$0")" && pwd)
WRAPPER_JAR="$APP_HOME/gradle/wrapper/gradle-wrapper.jar"

if [ -n "$JAVA_HOME" ] && [ -x "$JAVA_HOME/bin/java" ]; then
  JAVA_EXEC="$JAVA_HOME/bin/java"
else
  JAVA_EXEC=java
fi

exec "$JAVA_EXEC" ${DEFAULT_JVM_OPTS:-} -classpath "$WRAPPER_JAR" org.gradle.wrapper.GradleWrapperMain "$@"
